{
  "name": "hashless",
  "version": "1.0.10",
  "description": "Lightweight and efficient hashing & verification library optimized for serverless environments like AWS Lambda and Cloudflare Workers.",
  "module": "index.ts",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "bun build --target=node ./src/index.ts --outfile=dist/index.js && bun run build:declaration",
    "build:declaration": "tsc --emitDeclarationOnly --project tsconfig.types.json",
    "postbuild": "rimraf tsconfig.types.tsbuildinfo"
  },
  "devDependencies": {
    "bun-types": "latest",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jenil-Desai/Hashless.git"
  },
  "keywords": [
    "hashless",
    "hash",
    "typescript"
  ],
  "author": "Jenil Desai jenildev91@gmail.com",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14.0.0"
  }
}
